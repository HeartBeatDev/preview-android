<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
  <item>
    <shape android:shape="rectangle">
      <gradient
          android:angle="0"
          android:endColor="#4E473D"
          android:startColor="#82786F"
          android:type="linear"
          />
      <corners android:radius="78dp" />
    </shape>
  </item>

  <item
      android:bottom="3dp"
      android:left="3dp"
      android:right="3dp"
      android:top="3dp"
      >
